<h1><%#= t ".title" %></h1>
<h1><%= t "hello" %></h1>

<%= link_to "New product", new_product_path if authenticated? %>

<div id="products">
    <% @products.each do |product| %>
        <div>
            <a href="/products/<%= product.id %>">
                <%= product.name %>
            </a>
        </div>
    <% end %>
</div>